<Pop_up>:
    title:'Do you want to exit'
    title_color:1,0,1,1
    size_hint:0.3,0.2
    BoxLayout:
        padding:[20,20,20,20]
        spacing:dp(20)
        canvas.before:
            Color:
                rgba: 1, 0.65, 1, 0.65
            Rectangle:
                pos:self.pos
                size:self.size
        Button:
            text:"Yes"
            background_color:0.4, 0.2, 1, 0.3
            on_press:app.Exit()
        Button:
            text:"No"
            background_color:0.4, 0.2, 1, 0.3
            on_press:root.dismiss()
            
