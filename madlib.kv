Interface:
    Screen:
        name:"page1"
        canvas.before:
            Color:
                rgba: 0,0,0,0
            Rectangle:
                pos:self.pos
                size:self.size
                size:self.size
        FloatLayout:
            AnchorLayout:
                Button:
                    size_hint:None,None
                    anchor_x:'center'
                    anchor_y:'center'
                    size:300,300
                    background_normal:'img.png'
                    background_down:'img.png'
                    #border:30,30,30,30
            Button:
                text:"Start"
                font_size:dp(20)
                size_hint:0.3,0.08
                padding:[20,20,20,20]
                pos_hint:{'x':0.35,'y':0.2}
                background_normal:''
                background_color :"#1995AD"
                on_press:root.genre()
    Screen:
        name:"page2"
        canvas.before:
            Color:
                rgba: 1, 0.65, 1, 1
            Rectangle:
                pos:self.pos
                size:self.size
        FloatLayout:
            Button:
                text:"Exit"
                background_normal:''
                font_size:dp(20)
                size_hint_y:None
                size:self.texture_size
                size_hint:0.1,0.08
                pos_hint:{'right':0.99,'top':0.99}
                on_press:root.show()
                background_color:0.4, 0.2, 1, 0.3
            Label:
                text:"Choose Genre"
                text_size: self.size
                size_hint_y:None
                size:self.texture_size
                font_size:30
                pos_hint:{'x':0.4,'y':0.8}
                background_color: 1, 0.65, 1, 0.2
        AnchorLayout:
            anchor_x:'center'
            anchor_y:'center'
            GridLayout:
                cols:2
                size_hint:0.5,0.9
                padding:[50,200,50,50]
                border:(25,25,25,20)
                spacing:30
                Button: 
                    size_hint:0.1,0.5
                    text:'Adventure'
                    font_name:'shadowintolighter.ttf'
                    font_size:'33sp'
                    background_color:0.4, 0.2, 1, 0.3
                    background_normal:''
                    on_press:root.input_page_adven()  
                Button:
                    font_name:'shadowintolighter.ttf'
                    font_size:'33sp'
                    background_color:0.4, 0.2, 1, 0.3
                    background_normal:''
                    size_hint:0.1,0.5
                    text:'Fairytale' 
                    on_press:root.input_page_fairy()   
                Button:
                    font_name:'shadowintolighter.ttf'
                    font_size:'33sp'
                    background_color:0.4, 0.2, 1, 0.5
                    background_normal:''
                    size_hint:0.1,0.5
                    text:'Crime' 
                    on_press:root.input_page_crime() 
                Button:
                    font_name:'shadowintolighter.ttf'
                    font_size:'33sp'
                    background_color:0.4, 0.2, 1, 0.5
                    background_normal:''
                    size_hint:0.1,0.5
                    text:'Fanatcy'
                    on_press:root.input_page_fantacy()
                Button:
                    font_name:'shadowintolighter.ttf'
                    font_size:'33sp'
                    background_color:0.4, 0.2, 1, 0.7
                    background_normal:''
                    size_hint:0.1,0.5
                    text:'Horror'
                    on_press:root.input_page_detective() 
                Button:
                    font_name:'shadowintolighter.ttf'
                    font_size:'33sp'
                    background_color:0.4, 0.2, 1, 0.7
                    background_normal:''
                    size_hint:0.1,0.5
                    text:'Comedy' 
                    on_press:root.input_page_comedy()   
    Screen:
        name:"page3"
